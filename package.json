  "repository": {
    "type": "git",
    "url": "git+https://github.com/vwinterdev/env-validator-vite.git"
  },
  "author": "vwinterdev",
  "homepage": "https://github.com/vwinterdev/env-validator-vite#readme",
  "bugs": {
    "url": "https://github.com/vwinterdev/env-validator-vite/issues"
