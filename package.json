{
  "name": "@vwinterdev/env-validator-vite",
  "version": "1.0.0",
  "description": "Vite plugin for validating environment variables with built-in validators",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
